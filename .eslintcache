/Invoices/InvoiceList.js",["114"],"import \"./InvoiceList.css\";\nimport InvoiceItem from \"./InvoiceItem\";\nimport { getDueDate } from \"../../utils/utils\";\nimport EmptyInvoices from \"./EmptyInvoices\";\n\nfunction InvoiceList(props) {\n  if (props.invoices.length <= 0) {\n    return (\n      <EmptyInvoices\n        display={\"none\"}\n        header={\"There is nothing here\"}\n        text={\"Create on invoice by clicking the New button and get started\"}\n      />\n    );\n  }\n\n  return (\n    <div className=\" list\">\n      {props.invoices.map((invoice) => {\n        const amount = invoice.itemList.map((item) => item.price * item.qty);\n\n        if (amount.length <= 0) {\n          return;\n        }\n\n        const totalAmount = amount.reduce((a, b) => a + b);\n\n        const dueDate = getDueDate(invoice.invoiceDate, invoice.terms);\n\n        return (\n          <InvoiceItem\n            key={invoice.id}\n            id={invoice.id}\n            toName={invoice.to.name}\n            dueDate={dueDate}\n            totalAmount={`£${totalAmount}`}\n            status={invoice.status}\n          />\n        );\n      })}\n    </div>\n  );\n}\n\nexport default InvoiceList;\n","/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Form/NewForm.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Layout/Filters.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Form/Item.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Invoices/InvoiceItem.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/utils/utils.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/pages/invoice/InvoicePage.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/InvoiceDetail/InvoiceDetail.js",[],"/Users/emiomadavid/Desktop/Uche