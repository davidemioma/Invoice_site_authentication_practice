rc/components/Layout/Layout.js",["124"],"import Filters from \"./Filters\";\nimport \"./Layout.css\";\nimport { useSelector } from \"react-redux\";\nimport AuthContext, { useAuth } from \"../../store/auth-context\";\n\nfunction Layout(props) {\n  const authCtx = useAuth();\n\n  const noOfInvoices = useSelector((state) => state.allInvoice.totalInvoices);\n\n  const getFilterValue = (value) => {\n    props.getFilterValue(value);\n  };\n\n  const onLogoutHandler = () => {\n    authCtx.logout();\n  };\n\n  return (\n    <div className=\"container layout \">\n      <header className=\"header\">\n        <div className=\"invo\">\n          <span>Invoices</span>\n          <span className=\"amount\">{noOfInvoices} invoices</span>\n        </div>\n\n        <div className=\"options\">\n          <Filters onFilterValue={getFilterValue} />\n\n          <button onClick={props.openForm}>\n            <span>\n              <img src={\"/static/images/icon-plus.svg\"} alt=\"\" />\n            </span>\n            New\n          </button>\n\n          <button onClick={onLogoutHandler}>Logout</button>\n        </div>\n      </header>\n\n      <main>{props.children}</main>\n    </div>\n  );\n}\n\nexport default Layout;\n","/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Invoices/InvoiceList.js",["125"],"import \"./InvoiceList.css\";\nimport InvoiceItem from \"./InvoiceItem\";\nimport { getDueDate } from \"../../utils/utils\";\nimport EmptyInvoices from \"./EmptyInvoices\";\n\nfunction InvoiceList(props) {\n  if (props.invoices.length <= 0) {\n    return (\n      <EmptyInvoices\n        display={\"none\"}\n        header={\"There is nothing here\"}\n        text={\"Create on invoice by clicking the New button and get started\"}\n      />\n    );\n  }\n\n  return (\n    <div className=\" list\">\n      {props.invoices.map((invoice) => {\n        const amount = invoice.itemList.map((item) => item.price * item.qty);\n\n        if (amount.length <= 0) {\n          return;\n        }\n\n        const totalAmount = amount.reduce((a, b) => a + b);\n\n        const dueDate = getDueDate(invoice.invoiceDate, invoice.terms);\n\n        return (\n          <InvoiceItem\n            key={invoice.id}\n            id={invoice.id}\n            toName={invoice.to.name}\n            dueDate={dueDate}\n            totalAmount={`£${totalAmount}`}\n            status={invoice.status}\n          />\n        );\n      })}\n    </div>\n  );\n}\n\nexport default InvoiceList;\n","/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Form/NewForm.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Layout/Filters.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Form/Item.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Invoices/InvoiceItem.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/utils/utils.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/pages/invoice/InvoicePage.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/InvoiceDetail/InvoiceDetail.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Form/EditForm.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Form/EditItem.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/store/invoice-actions.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/store/auth-context.js",["126","127"],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/firebase.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Auth/SignUp.js",[],"/Users/emiomadavid/Desktop/Uche/React/Invoice_site_with_authentication/src/components/Aut